#include <stdio.h>
int main()
{
    int a=10;
    int b=20;
    int *p=&a;
    int *q=&b;
    printf("before swapping elements: %d %d\n",*p,*q);
    int swap;
    swap=*p;
    *p=*q;
    *q=swap;
    printf("after swapping elements: %d %d",*p,*q);
}

output:
before swapping elements: 10 20
after swapping elements: 20 10


#include <stdio.h>
int swap(int *p, int *q,int t)
{
   
    t=*p;
    *p=*q;
    *q=t;
    printf("after swapping elements: %d %d",*p,*q); 
}
int main()
{
    int a=10;
    int b=20;
    int *p=&a;
    int *q=&b;
    int t;
    printf("before swapping elements: %d %d\n",*p,*q);
    int (*fptr)(int *, int *,int t)=swap;
    fptr(p,q,t);
    
}

output:
before swapping elements: 10 20
after swapping elements: 20 10
