#include<stdio.h>
int multiply(int arr[3][3], int arr1[3][3],int arr2[3][3])
{
    
    for(int i=0;i<3;i++)
    {
        for(int j=0;j<3;j++)
        {
            *(*(arr2+i)+j)=*(*(arr+i)+j) * *(*(arr1+i)+j);
        }
    }
    for(int i=0;i<3;i++)
    {
        for(int j=0;j<3;j++)
        {
            printf("%d ",*(*(arr2+i)+j));
        }
        printf("\n");
}
}
int main()
{
    int arr[3][3]={{1,2,3},{4,5,6},{7,8,9}};
    int arr1[3][3]={{10,11,12},{13,14,15},{16,17,18}};
    int arr2[3][3];
    int (*fptr)(int arr[3][3], int arr1[3][3], int arr2[3][3])=multiply;
    fptr(arr,arr1,arr2);
    
}

output:
10 22 36 
52 70 90 
112 136 162 
