#include<stdio.h>
int duplicate(int a[], int l)
{
   int i=0,j=0,c=0,k=0;
   for(i=0;i<l;i++)
   {
        for(j=i+1;j<l;j++)
           {
               if(a[i]==a[j])
               {
                   c++;
                  break;
               }
           }
   }
   printf("%d",c);
   }
int main()
{
   int a[]={1,2,3,1,2,3,4,5};
   int l=sizeof(a)/sizeof(a[0]); 
   int (*fptr)(int [], int)=duplicate;
   fptr(a,l);
}

output:
3
