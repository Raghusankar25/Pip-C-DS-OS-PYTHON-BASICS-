#include<stdio.h>
int merge(int a[], int b[], int c[])
{
    int i=0;
    for(i=0;i<3;i++)
    {
        c[i]=a[i];
    }
    int k=i;
    for(i=0;i<3;i++,k++)
    {
        c[k]=b[i];
       //k++;
    }
    for(i=0;i<6;i++)
    {
        printf("%d ",c[i]);
    }
    
}
int main()
{
    int a[]={1,2,3};
    int b[]={4,5,6};
    int c[10];
    int (*fptr)(int [], int [], int [])=merge;
    fptr(a,b,c);
}

output:
1 2 3 4 5 6
