#include<stdio.h>
int copy(int *p,int *q)
{
     for(int i=0;i<5;i++)
    {
        q[i]=p[i];
        printf("%d ",q[i]);
    }
}
int main()
{
    int a[5]={10,20,30,40,50},i;
    int b[5];
    int *p=a;
    int *q=b;
    int (*fptr)(int *, int *)=copy;
    fptr(p,q);
    
}

output:
10 20 30 40 50 
