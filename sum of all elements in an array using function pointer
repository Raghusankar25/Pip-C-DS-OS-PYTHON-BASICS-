 #include <stdio.h>  
int res(int arr[], int sum, int len)  
{  
   
    for (int i = 0; i < len; i++) {   
       sum = sum + arr[i];  
    }    
    printf("Sum of all the elements of an array: %d", sum);  
    return 0;  
}  
int main()
{
     int arr[] = {1, 2, 3, 4, 5};   
    int sum = 0;  
    int len = sizeof(arr)/sizeof(arr[0]);  
    int (*fptr)(int [],int, int)=res;
    fptr(arr,sum,len);
}

output:
Sum of all the elements of an array: 15
