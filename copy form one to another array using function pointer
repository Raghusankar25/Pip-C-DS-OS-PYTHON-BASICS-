#include<stdio.h>
int copy(int a[],int l)
{
    int b[10],i=0;
    for(i=0;i<l;i++)
    {
        b[i]=a[i];
    }
    printf("the copy one array to another array:");
    for(i=0;i<l;i++)
    {
        printf("%d ",b[i]);
    }
}
int main()
{
    int a[]={1,2,3,4,5};
    int l=sizeof(a)/sizeof(a[0]);
    int (*fptr)(int [], int)=copy;
    fptr(a,l);
}

output:
the copy one array to another array:1 2 3 4 5 
