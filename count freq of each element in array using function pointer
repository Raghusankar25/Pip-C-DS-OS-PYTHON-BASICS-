#include<stdio.h>
int freq(int a[],int l)
{
    int i=0,j=0,c=0;
    for(i=0;i<l;i++)
    {
        c=1;
        if(a[i]>0)
        {
            for(j=i+1;j<l;j++)
            {
                if(a[i]==a[j])
                {
                    c++;
                    a[j]='\0';
                }
            }
            printf("%d | %d\n",a[i],c);
        }
    }
}
int main()
{
    int a[]={1,2,3,2,1,2,3,4,5};
    int l=sizeof(a)/sizeof(a[0]);
    int (*fptr)(int [], int)=freq;
    fptr(a,l);
}
output:
1 | 2
2 | 3
3 | 2
4 | 1
5 | 1
