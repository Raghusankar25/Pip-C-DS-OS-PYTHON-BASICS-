#include<stdio.h>
int swap(int *p, int *q)
{
    for(int i=0;i<5;i++)
    {
    int t;
    t=*(p+i);
    *(p+i)=*(q+i);
    *(q+i)=t;
    }
    for(int i=0;i<5;i++)
        printf("%d ",*(p+i));
    printf("\n");
    for(int i=0;i<5;i++)
        printf("%d ",*(q+i));
}
int main()
{
    int a[]={10,20,30,40,50};
    int b[]={60,70,80,90,100};
    int *p=a;
    int *q=b;
    int (*fptr)(int *, int *)=swap;
    fptr(p,q);
}

output:
60 70 80 90 100 
10 20 30 40 50 
