#include<stdio.h>
int delduplicate(int a[], int l)
{
   int i=0,j=0,c=0;
   for(i=0;i<l;i++)
   {
       for(j=0;j<l;j++)//   fptr(a,l);
// }
       {
           if(a[i]==a[j])
           {
               c++;
               break;
           }
       }
       if(i==j)
       {
           printf("%d  ",a[i]);
       }
   }
}
int main()
{
    int a[]={1,2,1,44,22,2,1}; 
   int l=sizeof(a)/sizeof(a[0]);
   int (*fptr)(int [], int)=delduplicate;
   fptr(a,l);
}

output:
1 2 44 22
