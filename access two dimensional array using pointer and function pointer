#include <stdio.h>    
void access2D(int arr[3][3], int *arr1[3][3])    
	{    
	    
    
	    // for (i=0;i<3;i++)    
	    // {    
	    //     for (j=0;j<3;j++)    
	    //     {    
	    //         printf("Enter a[%d][%d]: ",i,j);                
	    //         scanf("%d",&arr[i][j]);    
	    //     }    
	    // }    
	     for (int i = 0; i < 3; i++) {
	        for (int j = 0; j < 3; j++) {
	            arr1[i][j] = &arr[i][j];
	        }
	    }
	    printf("printing the elements:");     
	    for(int i=0;i<3;i++)    
	    {    
	        printf("\n");    
	        for (int j=0;j<3;j++)    
	        {    
	            printf("%d\t",*arr1[i][j]);    
	        }    
	    }    
	}
int main()
{
    int arr[3][3]={{1,2,3},{4,5,6},{7,8,9}}; 
	    int *arr1[3][3];
	    void (*fptr)(int [3][3], int *[3][3])=access2D;
	    fptr(arr,arr1);
}


output:
printing the elements:
1	2	3	
4	5	6	
7	8	9	
