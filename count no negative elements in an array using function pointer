#include<stdio.h>
int neg(int a[], int l)
{
   
    int i=0,c=0;
    for(i=0;i<l;i++)
    {
        if(a[i]<0)
        {
            c++;
        }
    }
    printf("the negative elements:%d",c);
}
int main()
{
    int a[]={1,2,3,4,5,-1,-2,-1,-2};
    int l=sizeof(a)/sizeof(a[0]);
    int (*fptr)(int [], int)=neg;
    fptr(a,l);
}

output:
the negative elements:4
