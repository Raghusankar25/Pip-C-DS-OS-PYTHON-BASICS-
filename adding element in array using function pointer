#include<stdio.h>
int addele(int a[], int l, int pos, int ele)
{
    int i;
    for(i=l-1;i>=pos;i--)
    {
        a[i]=a[i-1];
    }
    a[pos-1]=ele;
    for(i=0;i<l;i++)
    {
        printf("%d ",a[i]);
    }
}
int main()
{
   int a[]={1,2,3,4,5},pos=2,ele=9;
    int l=sizeof(a)/sizeof(a[0]);
    l++;
    int (*fptr)(int [], int, int, int)=addele;
    fptr(a,l,pos,ele);
}

output:
1 9 2 3 4 5
