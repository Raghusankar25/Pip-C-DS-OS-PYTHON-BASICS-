#include<stdio.h>  
int rev(int *ptr, int l)
{
   
   printf("\nElements of array in reverse order\n");  
    for(int i = 0; i < l; i++)  
        printf("%d\n", *ptr--);  
    return 0;  
}
int main()  
{  
    int a[]={10,20,30,40,50}; 
    int l=sizeof(a)/sizeof(a[0]);
    int *ptr=a;
    ptr = &a[l - 1];
    int (*fptr)(int *,int)=rev;
    fptr(ptr,l);
} 

output:
Elements of array in reverse order
50
40
30
20
10
