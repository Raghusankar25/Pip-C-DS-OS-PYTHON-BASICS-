#include<stdio.h>
int unique(int a[], int b[], int l)
{
  int i=0,j=0,c=0,k=0;
   for(i=0;i<l;i++)
   {
       c=1;
        for(j=i+1;j<l;j++)
           {
               if(a[i]==a[j])
               {
                   c++;
                  a[j]=-1;
               }
           }
           if(a[i]!=-1)
           {
            b[i]=c;   
           }
   }
   for(i=0;i<l;i++)
   {
       if(a[i]!=-1)
       {
           if(b[i]==1)
           {
               printf("%d ",a[i]);
           }
       }
   }
   }
  int main()
  {
      int a[]={1,2,3,1,2,3,4,5};
      int l=sizeof(a)/sizeof(a[0]);
      int b[l];
      int (*fptr)(int [], int [], int)=unique;
      fptr(a,b,l);
  }
  
  output:
4 5 
